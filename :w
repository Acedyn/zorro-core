package scheduling

import (
	"context"

	"github.com/Acedyn/zorro-core/internal/network"

	"google.golang.org/grpc"
)

type schedulingServer struct {
  UnimplementedSchedulingServer
}

func (server *schedulingServer) CommunicateCommands(Scheduling_CommunicateCommandsServer) error {
  return nil
}

func init() {
  RegisterSchedulingServer(network.GrpcServer(), schedulingServer{})
}

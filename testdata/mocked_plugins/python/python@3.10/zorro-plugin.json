{
  "env": {
    "PATH": {
      "append": ["./bin"]
    },
    "PYTHONPATH": {
      "append": [".", "./protos/"]
    }
  },
  "processors": [
    {
      "name": "python",
      "version": "3.10",
      "start_processor_template": "{{name}} -m zorro_python.processors.python_processor -i {{id}} -c{{#commands}} \"{{.}}\"{{/commands}}"
    }
  ],
  "tools": {
    "commands": [
      {
        "path": "./zorro_python/commands/log/log.py",
        "category": "python"
      }
    ]
  }
}
